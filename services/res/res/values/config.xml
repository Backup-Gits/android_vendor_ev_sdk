<resources>
    <!-- Defines external services to be started by the LineageSystemServer at boot. The service itself
         should publish as a binder services in its onStart -->
    <string-array name="config_externalVendorServices">
    </string-array>

    <!-- The LineageSystemServer class that is invoked from Android's SystemServer -->
    <string name="config_externalSystemServer" translatable="false">com.evervolv.platform.internal.VendorServer</string>

    <!-- Whether device has a screen with a higher aspect ratio -->
    <bool name="config_haveHigherAspectRatioScreen">false</bool>

    <!-- Path to the library that contains a device specific key handler -->
    <string-array name="config_deviceKeyHandlerLibs" translatable="false">
    </string-array>

     <!-- Name of that key handler class -->
    <string-array name="config_deviceKeyHandlerClasses" translatable="false">
    </string-array>
</resources>
